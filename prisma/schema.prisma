generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Producto {
  id String @id @default(cuid())
  Nombre String
  description String?
  tipo Tipo @default(COMESTIBLE)
  dueDate DateTime?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Tipo {
 BEBESTIBLE
 COMESTIBLE
}